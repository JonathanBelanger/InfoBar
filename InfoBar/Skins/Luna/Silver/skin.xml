<?xml version="1.0"?>
<skin name="Luna (Silver)" author="Microsoft" version="1.1" website="http://www.microsoft.com/">
	<background height="28" alpha="255" enableglass="false">
		<image file="background.png" sizingtype="tile" sizingmargins="0,14,0,4" contentmargins="2,2,2,2" />
		<text font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="255,0,0,0" yoffset="0" effect="none" />
	</background>
	<separator>
		<image file="separator.png" sizingtype="none" sizingmargins="0,0,0,0" />
	</separator>
	<verticalseparator>
		<image file="vertsep.png" sizingtype="tile" sizingmargins="0,0,0,0" />
	</verticalseparator>
	<button>
		<image file="button.png" sizingtype="stretch" sizingmargins="6,6,6,6" contentmargins="3,0,3,0" />
		<normaltext font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="255,0,0,0" yoffset="0" effect="none" />
		<hovertext font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="255,0,0,0" yoffset="0" effect="none" />
		<downtext font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="255,255,255,255" yoffset="0" effect="none" />
		<dropdownarrow file="arrow.png" />
	</button>
	<textbox>
		<image file="textbox.png" sizingtype="stretch" sizingmargins="6,6,6,6" contentmargins="4,1,4,0" />
		<text font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="255,255,255,255" yoffset="0" effect="none" />
		<inactivetext font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="192,255,255,255" yoffset="-1" effect="none" />
	</textbox>
	<tooltip enableglass="false">
		<image file="tooltip.png" mask="n/a" sizingtype="stretch" sizingmargins="4,4,8,8" contentmargins="5,5,9,9" />
		<text font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="255,0,0,0" yoffset="0" effect="none" />
		<separator>
			<image file="tooltip_separator.png" sizingtype="tile" sizingmargins="4,2,4,2" />
		</separator>
	</tooltip>
	<graph>
		<image file="textbox.png" sizingtype="stretch" sizingmargins="6,5,6,5" contentmargins="2,2,1,2" />
		<gridlines color="32,0,0,0"/>
	</graph>
	<menu>
		<background>
			<image file="menu.png" sizingtype="stretch" sizingmargins="6,2,2,2" contentmargins="5,0,0,0" />
		</background>
		<margin>
			<image file="menu_margin.png" sizingtype="stretch" sizingmargins="3,3,3,3" />
		</margin>
		<item>
			<image file="menu_item.png" sizingtype="tile" sizingmargins="0,0,0,0" contentmargins="1,2,5,2" />
			<normaltext font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="255,0,0,0" yoffset="2" effect="none" />
			<hovertext font="Tahoma" fontsize="8" fontbold="false" fontitalic="false" fontunderline="false" color="255,255,255,255" yoffset="2" effect="none" />
		</item>
		<separator>
			<image file="menu_separator.png" sizingtype="stretch" sizingmargins="30,2,3,2" />
		</separator>
		<arrow>
			<image file="menu_arrow.png" />
		</arrow>
		<checkradio>
			<image file="menu_checkradio.png" />
		</checkradio>
	</menu>
	<slider>
		<background>
			<image file="slider_back.png" sizingtype="tile" sizingmargins="1,1,1,1" contentmargins="0,0,0,0" />
		</background>
		<button>
			<image file="slider.png" sizingtype="none" sizingmargins="0,0,0,0" />
		</button>
	</slider>		
</skin>